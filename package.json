{
  "name": "hep-companion",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:ts": "tsc --noEmit",
    "lint:all": "npm run lint && npm run lint:ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "node scripts/run-integration-tests.js",
    "test:integration:ui": "node scripts/run-integration-tests.js --ui",
    "test:integration:debug": "node scripts/run-integration-tests.js --debug",
    "test:integration:auth": "node scripts/run-integration-tests.js --pattern=auth/*.spec.ts",
    "test:integration:exercises": "node scripts/run-integration-tests.js --pattern=exercises/*.spec.ts",
    "test:integration:organizations": "node scripts/run-integration-tests.js --pattern=organizations/*.spec.ts",
    "test:all": "npm run test && npm run test:integration",
    "migrate": "./apply-migrations.sh",
    "migrate:local": "npx supabase db push",
    "migrate:prod": "npx supabase db push",
    "supabase:login": "npx supabase login",
    "supabase:projects": "npx supabase projects list",
    "create-test-user": "node scripts/user/create-test-user.js",
    "fix-audit-logs": "node scripts/security/fix-audit-logs-policy.js",
    "check-env": "node scripts/utils/check-env.js",
    "fix-organization": "node scripts/database/fix-organization-constraint.js",
    "fix-organization-sql": "node scripts/database/sql-fix-organization-constraint.js",
    "deploy-security": "node scripts/security/deploy-security.js",
    "verify:phase2": "npx tsx scripts/verify-phase2.ts"
  },
