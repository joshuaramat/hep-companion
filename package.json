    "openai": "^4.95.0",
    "postcss": "^8.4.35",
    "postgres": "^3.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "zod": "^3.22.4"
